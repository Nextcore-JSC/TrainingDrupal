<?php

/**
 * Implements hook_preprocess_page().
 */
function lioa_preprocess_page(&$variables) {
  if ($node = \Drupal::routeMatch()->getParameter('node')) {
    if ($node->uid == 1) {
    $variables['fooder_bottom'] = 'Chúng tôi là nhà sản xuất Việt Nam';
    }
  }
  
}
